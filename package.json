  },
  "license": "MIT",
  "engines": {
    "staruml": ">=6.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "change-case-all": "^2.1.0",
    "copy-to-clipboard": "^3.3.3",
    "ejs": "^3.1.9",
    "joi": "^17.12.2",
    "json-beautify": "^1.1.1",
    "json-stringify-safe": "^5.0.1"
